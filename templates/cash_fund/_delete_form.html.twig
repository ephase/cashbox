<form method="post" action="{{ path('cash_fund_delete', {'id': cash_fund.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cash_fund.id) }}">
    <button class="btn">Delete</button>
</form>
